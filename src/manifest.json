{
  "name": "Shortcuts for Google™ Products",
  "version": "1.1.4",
  "manifest_version": 2,
  "short_name": "igshortcuts",
  "description": "Display all Google services in a single space-saving drop-down panel",
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "16": "data/icon16.png",
      "32": "data/icon32.png"
    },
    "default_popup": "data/popup/popup.html",
    "default_title": "Shortcuts for Google™ Products"
  },
  "background": {
    "page": "lib/chrome/background.html"
  },
  "icons": {
    "16": "data/icon16.png",
    "32": "data/icon32.png",
    "48": "data/icon48.png",
    "64": "data/icon64.png",
    "128": "data/icon128.png"
  }
}
